{
  "name": "clash-intelligence",
  "private": true,
  "version": "1.50.0",
  "_forceRebuild": "2025-01-15-19:45:00",
  "scripts": {
    "dev": "NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next dev -p 5050",
    "dev:alt": "NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next dev -p 5060",
    "dev:e2e": "NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next dev -p 5110",
    "dev:staging": "VERCEL_ENV=preview NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next dev -p 5050",
    "build": "NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next build",
    "build:staging": "VERCEL_ENV=preview NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next build",
    "start": "NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next start -p 5050",
    "start:alt": "NEXT_PUBLIC_APP_VERSION=$(node -p \"require('./package.json').version\") next start -p 5060",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:screenshots": "playwright test tests/e2e/screenshots.spec.ts --project=chromium",
    "lint": "next lint",
    "deploy:staging": "vercel --target preview",
    "deploy:prod": "vercel --target production",
    "env:check": "node scripts/check-env.js",
    "ingest:run": "tsx scripts/ingest/run.ts",
    "ingest:mac": "tsx scripts/mac-ingestion.ts",
    "ingest:mac:all": "tsx scripts/mac-ingestion.ts --all",
    "ingest:mac:setup": "bash scripts/setup-mac-ingestion.sh",
    "ingest:mac:status": "launchctl list com.clashintelligence.ingestion || echo 'Not running'",
    "ingest:mac:check": "bash scripts/check-ingestion-status.sh",
    "ingest:mac:stop": "launchctl unload ~/Library/LaunchAgents/com.clashintelligence.ingestion.plist 2>/dev/null || echo 'Not loaded'",
    "ingest:mac:start": "launchctl load ~/Library/LaunchAgents/com.clashintelligence.ingestion.plist 2>/dev/null || echo 'Already loaded or not installed'",
    "ingest:mac:onetime": "bash scripts/setup-one-time-ingestion.sh",
    "ingest:mac:onetime:at": "bash scripts/setup-one-time-ingestion-at.sh",
    "setup:storage": "tsx scripts/setup-supabase-storage.ts",
    "backfill-enriched-data": "tsx scripts/backfill-enriched-data.ts",
    "backfill:player-day": "tsx scripts/backfill-player-day.ts",
    "backfill:player-day-clan": "tsx scripts/backfill-player-day-from-clan.ts",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "commit": "npm run release:minor"
  },
  "dependencies": {
    "@supabase/mcp-server-supabase": "^0.5.5",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/node-cron": "^3.0.11",
    "@upstash/ratelimit": "^1.2.1",
    "@upstash/redis": "^1.35.3",
    "@vercel/blob": "^1.1.1",
    "@vercel/kv": "^3.0.0",
    "axios": "^1.11.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cors": "^2.8.5",
    "date-fns": "4.1.0",
    "express": "^5.1.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "lucide-react": "0.452.0",
    "next": "14.2.5",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.10",
    "openai": "^5.19.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.15.4",
    "swr": "2.3.6",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "undici": "^7.15.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.9",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "17.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.4.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.5.4",
    "@playwright/test": "^1.48.2"
  }
}
