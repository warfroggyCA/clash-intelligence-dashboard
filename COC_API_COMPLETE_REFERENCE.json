{
  "_meta": {
    "title": "Clash of Clans API - Complete Data Reference",
    "description": "Comprehensive documentation of ALL data points, endpoints, and fields available in the CoC API",
    "api_version": "v1",
    "base_url": "https://api.clashofclans.com/v1",
    "authentication": "Bearer token (JWT) in Authorization header",
    "created": "2025-01-26",
    "rate_limits": {
      "developer_silver": "Throttled per IP/token",
      "allowed_ips": "Must whitelist IPs in developer portal"
    }
  },
  
  "endpoints": {
    "clans": {
      "search": {
        "method": "GET",
        "path": "/clans",
        "description": "Search for clans by name or filter criteria",
        "query_parameters": {
          "name": "Clan name search (3+ characters)",
          "warFrequency": "Filter by war frequency (always, moreThanOncePerWeek, oncePerWeek, lessThanOncePerWeek, never, unknown)",
          "locationId": "Filter by location ID",
          "minMembers": "Minimum number of members",
          "maxMembers": "Maximum number of members",
          "minClanPoints": "Minimum clan points",
          "minClanLevel": "Minimum clan level",
          "limit": "Number of results (default 10, max 100)",
          "after": "Pagination cursor",
          "before": "Pagination cursor",
          "labelIds": "Comma-separated list of label IDs"
        },
        "returns": "Paginated list of clan summaries"
      },
      
      "get_clan": {
        "method": "GET",
        "path": "/clans/{clanTag}",
        "description": "Get detailed information about a specific clan",
        "parameters": {
          "clanTag": "Clan tag with URL-encoded # (e.g., %232PR8R8V8P)"
        },
        "response_fields": {
          "tag": "string - Clan tag (e.g., #2PR8R8V8P)",
          "name": "string - Clan name",
          "type": "string - Clan type (open, inviteOnly, closed)",
          "description": "string - Clan description text",
          "location": {
            "id": "number - Location ID",
            "name": "string - Location name",
            "isCountry": "boolean - Is it a country or region",
            "countryCode": "string - 2-letter country code"
          },
          "isFamilyFriendly": "boolean - Family friendly flag",
          "badgeUrls": {
            "small": "string - URL to small badge image",
            "medium": "string - URL to medium badge image",
            "large": "string - URL to large badge image"
          },
          "clanLevel": "number - Clan level (1-20+)",
          "clanPoints": "number - Total clan points",
          "clanVersusPoints": "number - Builder base clan points",
          "clanCapitalPoints": "number - Clan capital points",
          "capitalLeague": {
            "id": "number - Capital league ID",
            "name": "string - Capital league name"
          },
          "requiredTrophies": "number - Minimum trophies to join",
          "requiredTownhallLevel": "number - Minimum TH level to join",
          "requiredVersusTrophies": "number - Minimum builder trophies",
          "warFrequency": "string - War frequency setting",
          "warWinStreak": "number - Current war win streak",
          "warWins": "number - Total war wins",
          "warTies": "number - Total war ties",
          "warLosses": "number - Total war losses",
          "isWarLogPublic": "boolean - Is war log public",
          "warLeague": {
            "id": "number - War league ID",
            "name": "string - War league name (e.g., Crystal League II)"
          },
          "members": "number - Current member count",
          "memberList": [
            {
              "tag": "string - Player tag",
              "name": "string - Player name",
              "role": "string - member, admin, coLeader, leader",
              "expLevel": "number - Experience level",
              "league": {
                "id": "number - League ID",
                "name": "string - League name",
                "iconUrls": "object - League icon URLs"
              },
              "trophies": "number - Current trophies",
              "versusTrophies": "number - Builder base trophies",
              "clanRank": "number - Rank in clan (by trophies)",
              "previousClanRank": "number - Previous season rank",
              "donations": "number - Troops donated this season",
              "donationsReceived": "number - Troops received this season",
              "playerHouse": {
                "elements": "array - Player house decoration elements"
              }
            }
          ],
          "labels": [
            {
              "id": "number - Label ID",
              "name": "string - Label name",
              "iconUrls": "object - Label icon URLs"
            }
          ],
          "chatLanguage": {
            "id": "number - Language ID",
            "name": "string - Language name",
            "languageCode": "string - 2-letter language code"
          },
          "clanCapital": {
            "capitalHallLevel": "number - Capital hall level (1-10)",
            "districts": [
              {
                "id": "number - District ID",
                "name": "string - District name",
                "districtHallLevel": "number - District hall level"
              }
            ]
          }
        }
      },
      
      "get_clan_members": {
        "method": "GET",
        "path": "/clans/{clanTag}/members",
        "description": "Get list of clan members (same data as memberList in get_clan)",
        "note": "Returns same member data as /clans/{clanTag} but paginated"
      },
      
      "get_clan_warlog": {
        "method": "GET",
        "path": "/clans/{clanTag}/warlog",
        "description": "Get clan's war log history",
        "requires": "Public war log enabled",
        "response_fields": {
          "items": [
            {
              "result": "string - win, lose, tie, or null (ongoing)",
              "endTime": "string - ISO 8601 timestamp",
              "teamSize": "number - Players per side",
              "attacksPerMember": "number - Attacks allowed per member",
              "clan": {
                "tag": "string - Clan tag",
                "name": "string - Clan name",
                "badgeUrls": "object - Badge URLs",
                "clanLevel": "number - Clan level",
                "attacks": "number - Total attacks used",
                "stars": "number - Total stars earned",
                "destructionPercentage": "number - Total destruction %",
                "expEarned": "number - XP earned from war"
              },
              "opponent": {
                "tag": "string - Opponent clan tag (may be hidden)",
                "name": "string - Opponent clan name",
                "badgeUrls": "object - Badge URLs",
                "clanLevel": "number - Clan level",
                "stars": "number - Total stars earned",
                "destructionPercentage": "number - Total destruction %"
              }
            }
          ],
          "paging": {
            "cursors": "object - Pagination cursors"
          }
        }
      },
      
      "get_current_war": {
        "method": "GET",
        "path": "/clans/{clanTag}/currentwar",
        "description": "Get current war information (regular war, not CWL)",
        "response_fields": {
          "state": "string - notInWar, preparation, inWar, warEnded",
          "teamSize": "number - Players per side",
          "attacksPerMember": "number - Attacks per member (usually 2)",
          "preparationStartTime": "string - ISO 8601 timestamp",
          "startTime": "string - ISO 8601 timestamp",
          "endTime": "string - ISO 8601 timestamp",
          "clan": {
            "tag": "string - Clan tag",
            "name": "string - Clan name",
            "badgeUrls": "object - Badge URLs",
            "clanLevel": "number - Clan level",
            "attacks": "number - Total attacks used",
            "stars": "number - Total stars earned",
            "destructionPercentage": "number - Total destruction %",
            "members": [
              {
                "tag": "string - Player tag",
                "name": "string - Player name",
                "townhallLevel": "number - TH level",
                "mapPosition": "number - War map position (1-50)",
                "opponentAttacks": "number - Times attacked by opponent",
                "attacks": [
                  {
                    "order": "number - Attack sequence number",
                    "attackerTag": "string - Attacker's player tag",
                    "defenderTag": "string - Defender's player tag",
                    "stars": "number - Stars earned (0-3)",
                    "destructionPercentage": "number - Destruction % (0-100)",
                    "duration": "number - Attack duration in seconds"
                  }
                ],
                "bestOpponentAttack": {
                  "order": "number",
                  "attackerTag": "string",
                  "defenderTag": "string",
                  "stars": "number",
                  "destructionPercentage": "number",
                  "duration": "number"
                }
              }
            ]
          },
          "opponent": "object - Same structure as clan"
        }
      },
      
      "get_cwl_group": {
        "method": "GET",
        "path": "/clans/{clanTag}/currentwar/leaguegroup",
        "description": "Get Clan War League group information",
        "note": "Only available during CWL season",
        "response_fields": {
          "state": "string - notInWar, preparation, war, ended",
          "season": "string - Season identifier (e.g., 2025-01)",
          "clans": [
            {
              "tag": "string - Clan tag",
              "name": "string - Clan name",
              "clanLevel": "number - Clan level",
              "badgeUrls": "object - Badge URLs",
              "members": [
                {
                  "tag": "string - Player tag",
                  "name": "string - Player name",
                  "townHallLevel": "number - TH level"
                }
              ]
            }
          ],
          "rounds": [
            {
              "warTags": [
                "string - War tag IDs for this round"
              ]
            }
          ]
        }
      },
      
      "get_cwl_war": {
        "method": "GET",
        "path": "/clanwarleagues/wars/{warTag}",
        "description": "Get specific CWL war by war tag",
        "note": "War tags obtained from leaguegroup rounds",
        "response_fields": "Similar structure to /currentwar but for CWL wars"
      },
      
      "get_capital_raid_seasons": {
        "method": "GET",
        "path": "/clans/{clanTag}/capitalraidseasons",
        "description": "Get clan's capital raid weekend history",
        "query_parameters": {
          "limit": "Number of seasons to return"
        },
        "response_fields": {
          "items": [
            {
              "state": "string - ongoing or ended",
              "startTime": "string - ISO 8601 timestamp",
              "endTime": "string - ISO 8601 timestamp",
              "capitalTotalLoot": "number - Total capital gold looted",
              "raidsCompleted": "number - Number of raids completed",
              "totalAttacks": "number - Total attacks made",
              "enemyDistrictsDestroyed": "number - Enemy districts destroyed",
              "offensiveReward": "number - Offensive raid medals earned",
              "defensiveReward": "number - Defensive raid medals earned",
              "members": [
                {
                  "tag": "string - Player tag",
                  "name": "string - Player name",
                  "attacks": "number - Attacks made",
                  "attackLimit": "number - Attack limit",
                  "bonusAttackLimit": "number - Bonus attacks available",
                  "capitalResourcesLooted": "number - Capital gold looted"
                }
              ],
              "attackLog": [
                {
                  "attacker": {
                    "tag": "string - Attacker tag",
                    "name": "string - Attacker name"
                  },
                  "defenderTag": "string - Defender clan tag",
                  "defenderName": "string - Defender clan name",
                  "districts": [
                    {
                      "id": "number - District ID",
                      "name": "string - District name",
                      "stars": "number - Stars earned",
                      "destructionPercent": "number - Destruction %",
                      "attacks": "number - Attacks on this district"
                    }
                  ]
                }
              ],
              "defenseLog": [
                {
                  "attacker": {
                    "tag": "string - Attacker clan tag",
                    "name": "string - Attacker clan name"
                  },
                  "attackCount": "number - Total attacks received",
                  "districtCount": "number - Districts attacked",
                  "districtsDestroyed": "number - Districts destroyed",
                  "districts": [
                    {
                      "id": "number - District ID",
                      "name": "string - District name",
                      "stars": "number - Stars lost",
                      "destructionPercent": "number - Destruction %",
                      "attackCount": "number - Attacks received"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    },
    
    "players": {
      "get_player": {
        "method": "GET",
        "path": "/players/{playerTag}",
        "description": "Get complete player profile information",
        "parameters": {
          "playerTag": "Player tag with URL-encoded # (e.g., %23VGQVRLRL)"
        },
        "response_fields": {
          "tag": "string - Player tag",
          "name": "string - Player name",
          "townHallLevel": "number - Town Hall level (1-16)",
          "townHallWeaponLevel": "number - Town Hall weapon level",
          "expLevel": "number - Experience level",
          "trophies": "number - Current trophies",
          "bestTrophies": "number - All-time highest trophies",
          "warStars": "number - Total war stars earned",
          "attackWins": "number - Total attack wins",
          "defenseWins": "number - Total defense wins",
          "builderHallLevel": "number - Builder Hall level (1-10)",
          "builderBaseTrophies": "number - Builder base trophies",
          "bestBuilderBaseTrophies": "number - Best builder trophies",
          "role": "string - member, admin, coLeader, leader (if in clan)",
          "warPreference": "string - in or out",
          "donations": "number - Troops donated this season",
          "donationsReceived": "number - Troops received this season",
          "clanCapitalContributions": "number - Capital gold contributed",
          "clan": {
            "tag": "string - Clan tag",
            "name": "string - Clan name",
            "clanLevel": "number - Clan level",
            "badgeUrls": "object - Badge URLs"
          },
          "league": {
            "id": "number - League ID",
            "name": "string - League name",
            "iconUrls": {
              "small": "string - Small icon URL",
              "tiny": "string - Tiny icon URL",
              "medium": "string - Medium icon URL"
            }
          },
          "builderBaseLeague": {
            "id": "number - Builder league ID",
            "name": "string - Builder league name"
          },
          "achievements": [
            {
              "name": "string - Achievement name",
              "stars": "number - Stars earned (0-3)",
              "value": "number - Current progress value",
              "target": "number - Target value for completion",
              "info": "string - Achievement description",
              "completionInfo": "string - Completion description",
              "village": "string - home or builderBase"
            }
          ],
          "playerHouse": {
            "elements": [
              {
                "id": "number - Element ID",
                "type": "string - Element type (ground, walls, roof, decoration)"
              }
            ]
          },
          "labels": [
            {
              "id": "number - Label ID",
              "name": "string - Label name",
              "iconUrls": "object - Label icon URLs"
            }
          ],
          "troops": [
            {
              "name": "string - Troop name (e.g., Barbarian)",
              "level": "number - Current level",
              "maxLevel": "number - Max level for player's TH",
              "village": "string - home or builderBase",
              "superTroopIsActive": "boolean - Is super troop active (7 days)"
            }
          ],
          "heroes": [
            {
              "name": "string - Hero name (e.g., Barbarian King)",
              "level": "number - Current level",
              "maxLevel": "number - Max level for player's TH",
              "village": "string - home or builderBase",
              "equipment": [
                {
                  "name": "string - Equipment name",
                  "level": "number - Equipment level",
                  "maxLevel": "number - Max equipment level",
                  "village": "string - home"
                }
              ]
            }
          ],
          "heroEquipment": [
            {
              "name": "string - Equipment name",
              "level": "number - Current level",
              "maxLevel": "number - Max level",
              "village": "string - home"
            }
          ],
          "spells": [
            {
              "name": "string - Spell name (e.g., Lightning Spell)",
              "level": "number - Current level",
              "maxLevel": "number - Max level for player's TH",
              "village": "string - home or builderBase"
            }
          ],
          "legendStatistics": {
            "legendTrophies": "number - Legend league trophies",
            "bestSeason": {
              "id": "string - Season ID",
              "rank": "number - Global rank",
              "trophies": "number - Trophies earned"
            },
            "currentSeason": {
              "rank": "number - Current rank",
              "trophies": "number - Current trophies"
            },
            "previousSeason": {
              "id": "string - Season ID",
              "rank": "number - Final rank",
              "trophies": "number - Final trophies"
            }
          }
        }
      },
      
      "verify_player_token": {
        "method": "POST",
        "path": "/players/{playerTag}/verifytoken",
        "description": "Verify player API token (for linking accounts)",
        "body": {
          "token": "string - Player API token from in-game settings"
        },
        "response": {
          "status": "string - ok or invalid",
          "tag": "string - Player tag"
        }
      }
    },
    
    "leagues": {
      "list_leagues": {
        "method": "GET",
        "path": "/leagues",
        "description": "Get list of all trophy leagues",
        "response_fields": {
          "items": [
            {
              "id": "number - League ID (29000000-29000021)",
              "name": "string - League name (Unranked through Legend)",
              "iconUrls": {
                "small": "string",
                "tiny": "string",
                "medium": "string"
              }
            }
          ]
        }
      },
      
      "get_league": {
        "method": "GET",
        "path": "/leagues/{leagueId}",
        "description": "Get specific league information",
        "response": "Same as league object in list_leagues"
      },
      
      "get_league_seasons": {
        "method": "GET",
        "path": "/leagues/{leagueId}/seasons",
        "description": "Get available seasons for a league (Legend only)",
        "note": "Only works for Legend League (ID: 29000022)",
        "response_fields": {
          "items": [
            {
              "id": "string - Season ID (YYYY-MM format)"
            }
          ]
        }
      },
      
      "get_league_season_rankings": {
        "method": "GET",
        "path": "/leagues/{leagueId}/seasons/{seasonId}",
        "description": "Get player rankings for a league season",
        "note": "Only works for Legend League",
        "query_parameters": {
          "limit": "Results per page (max 200)",
          "after": "Pagination cursor",
          "before": "Pagination cursor"
        },
        "response_fields": {
          "items": [
            {
              "tag": "string - Player tag",
              "name": "string - Player name",
              "expLevel": "number - Experience level",
              "trophies": "number - Season trophies",
              "attackWins": "number - Attack wins",
              "defenseWins": "number - Defense wins",
              "rank": "number - Global rank",
              "previousRank": "number - Previous rank",
              "clan": {
                "tag": "string - Clan tag",
                "name": "string - Clan name",
                "badgeUrls": "object"
              }
            }
          ]
        }
      }
    },
    
